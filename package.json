{
  "name": "athanor",
  "productName": "Athanor",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "build:mcp": "tsc -p tsconfig.mcp.json && cd dist/mcp && echo '{\"name\":\"athanor-mcp\",\"private\":true}' > package.json && npm install --omit=dev better-sqlite3 kysely @modelcontextprotocol/sdk zod"
  },
  "keywords": [],
  "author": {
    "name": "kol22",
    "email": "kol.prue@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/plugin-webpack": "^7.11.1",
    "@electron/fuses": "^1.8.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "css-loader": "^6.11.0",
    "electron": "40.2.1",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "node-loader": "^2.1.0",
    "prettier": "^3.8.1",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.4",
    "typescript": "~5.4"
  },
  "dependencies": {
    "@fontsource-variable/geist": "^5.2.8",
    "@fontsource-variable/geist-mono": "^5.2.7",
    "@fontsource/instrument-serif": "^5.2.8",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@tailwindcss/postcss": "^4.1.18",
    "better-sqlite3": "^12.6.2",
    "electron-squirrel-startup": "^1.0.1",
    "geist": "^1.5.1",
    "kysely": "^0.28.11",
    "lucide-react": "^0.563.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4.1.18",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  }
}
